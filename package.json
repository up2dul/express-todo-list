{
  "name": "devcode-todolist-be",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "dev": "nodemon",
    "prepare": "husky install",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "check": "biome check --apply ./src",
    "check-types": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "keywords": [],
  "author": "Abdul<up2dul@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/up2dul/express-todo-list.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.5"
  }
}
